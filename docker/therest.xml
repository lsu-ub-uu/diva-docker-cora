<Context>
	<Parameter name="dependencyProviderClassName"
		value="se.uu.ub.cora.diva.DivaDependencyProvider" override="false" />
	<Parameter name="gatekeeperURL" value="http://gatekeeper:8080/gatekeeper/"
		override="false" />
	<Parameter name="storageOnDiskClassName" value="se.uu.ub.cora.storage.RecordStorageInMemoryReadFromDisk"
		override="false" />	  
	<Parameter name="storageOnDiskBasePath" value="/mnt/data/basicstorage/"
		override="false" />
	<Parameter name="solrURL" value="http://solr:8983/solr/coracore"
		override="false" />
	<Parameter name="theRestPublicPathToSystem" value="/diva/rest/"
		override="false" />
		<Parameter name="mixedStorageClassName" value="se.uu.ub.cora.diva.mixedstorage.DivaMixedRecordStorage"
		override="false" />	  
	<Parameter name="divaFedoraToCoraStorageClassName" value="se.uu.ub.cora.diva.tocorastorage.fedora.DivaToCoraRecordStorage"
		override="false" />	  
	<Parameter name="fedoraURL" value="http://diva-cora-fedora:8088/fedora/"
		override="false" />
		
		<Parameter name="divaDbToCoraStorageClassName" value="se.uu.ub.cora.diva.tocorastorage.db.DivaDbToCoraRecordStorage"
			override="false" />
		<Parameter name="databaseLookupName" value="java:/comp/env/jdbc/postgres"
			override="false" />
		<Resource name="jdbc/postgres" auth="Container" type="javax.sql.DataSource"
			driverClassName="org.postgresql.Driver"
			url="jdbc:postgresql://diva-cora-docker-postgresql:5432/diva"
			username="diva" password="diva" maxTotal="20" maxIdle="10"
			maxWaitMillis="-1" />
</Context>